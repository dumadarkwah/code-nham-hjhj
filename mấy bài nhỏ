#include <iostream>
#include <array>
using namespace std;


int main() {
	array<int, 5> arr;
	array<string, 10> arr2;

	for (int i = 0; i < arr2.size();i++) {
		cout << "arr2[" << i + 1 << "]: ";
		getline(cin, arr2[i]);
	}
cin.ignore();
	cout << "Cac gia tri: \n";
	for (string s : arr2) {
		cout << " - " << s << endl;
	}

	return 0;
}
