#include <iostream>
using namespace std;

int main () {
int n;
cout<<"nhap so nguyen: ";
cin>>n;

if (n<2) {
cout<<"NO";
return 0; }

bool isPrime = true;

for (int i = 2; i <= sqrt(n); i++) {
if (n % i == 0) {
isPrime = false;
break; }
}
cout<<(isPrime ? "YES" : "NO") ;
return 0; }

#include <iostream>
using namespace std; 

int main () {
long long n; 
cout<<"Nhap so n: ";
cin>>n;

int sum = 0;
while (n>0) {
int digit = n % 10;
if (digit % 3 == 0) sum += digit;
n /= 10; }
cout<<sum;
return 0; }
