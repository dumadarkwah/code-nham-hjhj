#include <iostream>
#include <iomanip>
using namespace std;

int main () {
double a, b, c;
cin>>a>>b>>c;

double tu = a*a + b*b + c*c + a*b + b*c + a*c;
double mau = a + b + c;

if (mau == 0) {
cout<<"Khong the chia 0"; }

else { double kq = tu/mau;
cout<<fixed<<setprecision(5)<<kq; }

return 0; }
