// next_combination.cpp
// Input: n=5, k=3
// comb: 1 3 5 â†’ next: 2 3 4
// Output next combination

#include <bits/stdc++.h>
using namespace std;

int main() {
    int n,k; cin >> n >> k;
    vector<int> a(k);
    for (int i=0;i<k;i++) cin >> a[i];
    int i=k-1;
    while(i>=0 && a[i]==n-k+i+1) i--;
    if(i<0) cout<<"No next combination\n";
    else {
        a[i]++;
        for(int j=i+1;j<k;j++) a[j]=a[j-1]+1;
        for(int x:a) cout<<x<<' ';
    }
}
