// next_combination.cpp
// Input: n=5, k=3
// comb: 1 3 5 â†’ next: 2 3 4
// Output next combination

#include <bits/stdc++.h>
using namespace std;

int main() {
    int n,k; cin >> n >> k;
    vector<int> a(k);
    for (int i=0;i<k;i++) cin >> a[i];
    int i=k-1;
    while(i>=0 && a[i]==n-k+i+1) i--;
    if(i<0) cout<<"No next combination\n";
    else {
        a[i]++;
        for(int j=i+1;j<k;j++) a[j]=a[j-1]+1;
        for(int x:a) cout<<x<<' ';
    }
}


// knapsack.cpp
// Input:
// 4 7
// 6 13
// 4 8
// 3 6
// 5 12
// Output: 14

#include <bits/stdc++.h>
using namespace std;

int main() {
    int n, W; cin >> n >> W;
    vector<int> wt(n+1), val(n+1);
    for(int i=1;i<=n;i++) cin >> wt[i] >> val[i];
    vector<vector<int>> dp(n+1, vector<int>(W+1,0));
    for(int i=1;i<=n;i++){
        for(int w=0;w<=W;w++){
            dp[i][w]=dp[i-1][w];
            if(w>=wt[i]) dp[i][w]=max(dp[i][w],dp[i-1][w-wt[i]]+val[i]);
        }
    }
    cout << dp[n][W] << "\n";
}
