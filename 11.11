#include <bits/stdc++.h>
using namespace std;

struct Student {
    string id;
    string name;
    double gpa;
};

bool cmp(const Student &a, const Student &b) {
    if (a.gpa != b.gpa) return a.gpa > b.gpa;
    return a.id < b.id;
}

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    int n; cin >> n;
    vector<Student> st(n);
    for (int i = 0; i < n; ++i)
        cin >> st[i].id >> ws, getline(cin, st[i].name), cin >> st[i].gpa;
    sort(st.begin(), st.end(), cmp);

    cout << "\n=== Sorted by GPA ===\n";
    for (auto &s : st)
        cout << s.id << " - " << s.name << " - " << s.gpa << "\n";

    cout << "\nSearch by ID: ";
    string x; cin >> x;
    int idx = -1;
    for (int i = 0; i < n; ++i)
        if (st[i].id == x) idx = i;
    if (idx == -1) cout << "Not found\n";
    else cout << "Found: " << st[idx].name << " GPA: " << st[idx].gpa << "\n";
}

// decode_string.cpp
// Input: s = "3[a2[b]]"
// Output: "abbabbabb"

#include <bits/stdc++.h>
using namespace std;

string decode(string s) {
    stack<int> num;
    stack<string> str;
    string cur = "";
    int n = 0;
    for (char c : s) {
        if (isdigit(c)) n = n * 10 + (c - '0');
        else if (c == '[') {
            num.push(n);
            str.push(cur);
            n = 0; cur = "";
        } else if (c == ']') {
            int cnt = num.top(); num.pop();
            string prev = str.top(); str.pop();
            string tmp = "";
            while (cnt--) tmp += cur;
            cur = prev + tmp;
        } else cur += c;
    }
    return cur;
}

int main() {
    string s; cin >> s;
    cout << decode(s) << "\n";
}

// dijkstra.cpp
// Input:
// 5 6
// 1 2 2
// 1 3 4
// 2 3 1
// 2 4 7
// 3 5 3
// 4 5 1
// Start: 1

#include <bits/stdc++.h>
using namespace std;
const int INF = 1e9;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    int n, m; cin >> n >> m;
    vector<vector<pair<int,int>>> adj(n+1);
    for (int i = 0; i < m; ++i) {
        int u,v,w; cin >> u >> v >> w;
        adj[u].push_back({v,w});
        adj[v].push_back({u,w});
    }
    int start; cin >> start;

    vector<int> dist(n+1, INF);
    dist[start] = 0;
    priority_queue<pair<int,int>, vector<pair<int,int>>, greater<>> pq;
    pq.push({0, start});
    while (!pq.empty()) {
        auto [d,u] = pq.top(); pq.pop();
        if (d != dist[u]) continue;
        for (auto [v,w] : adj[u]) {
            if (dist[v] > d + w) {
                dist[v] = d + w;
                pq.push({dist[v], v});
            }
        }
    }

    for (int i = 1; i <= n; ++i)
        cout << "Distance " << start << " -> " << i << ": " 
             << (dist[i] == INF ? -1 : dist[i]) << "\n";
}
