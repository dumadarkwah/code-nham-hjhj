#include <iostream>
#include <iomanip>
using namespace std;

int main () {
    int h, m;
    cin>>h>>m;
    string chuKy;
    if (h == 0) {
        h = 12;
        chuKy = "AM";
    } else if (h < 12) {
        chuKy = "PM";
    } else {
        h -= 12; 
        chuKy = "PM";
    }
    cout<<h<<":"<<setw(2)<<setfill('0')<<m<<" "<<chuKy;
    return 0;
}
